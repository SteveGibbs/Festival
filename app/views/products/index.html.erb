

<h1>TICKET PURCHASE PAGE UNDER CONSTRUCTION</h1>

<% @products.each do |item| %>
<%=item.product_name%>
<%=item.product_desc1%>
<%=number_to_currency(item.price)%>
<br>
<%= button_to "Admin Edit", edit_product_path(item), method: :get %>
<%=button_to "Buy", items_path(:product_id => item.id)%>
<%end%>
